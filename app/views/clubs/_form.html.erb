<div class="container-fluid">
  <%= simple_form_for @club do |f| %>
    <%= f.input :name %>
    <%= f.input :brand %>
    <%= f.collection_select :bag_id, current_user.bags, :id, :title, prompt: "Please pick the bag that this club belongs to:" %>
    <br>
    <br>
    <%= f.button :submit %>
  <% end %>
</div>