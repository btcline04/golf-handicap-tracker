<div id="bag-form" class="container-fluid">
  <%= simple_form_for @club do |f| %>
    <%= f.input :name %>
    <%= f.input :brand %>
    <%= f.collection_select :bag_id, current_user.bags, :id, :title, prompt: "Please pick the bag that this club belongs to:" %>
    <br>
    <br>
    <%= button_tag(type: 'submit', class: "btn btn-primary") do %> Create Club <% end %>  
  <% end %>
</div>