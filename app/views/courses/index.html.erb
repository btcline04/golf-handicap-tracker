<div class="container-fluid">
  <h1>My Courses</h1>
    <% @courses.each do |c| %>
      <p><%= link_to c do %></p>
      <p><%= c.name %></p>
      <% end %>
      <p><%= c.location %></p>
      <p>Par <%= c.par %></p>
    <% end %>
  <br>
  <%= link_to "Create a new course", new_course_path %>
</div>