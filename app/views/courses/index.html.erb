<div class="container-fluid">
  <h1>My Courses</h1>
    <% @courses.each do |c| %>
      <p><%= link_to c do %></p>
      <p><%= c.name %></p>
      <% end %>
      <p><%= c.location %></p>
      <p>Par <%= c.par %></p>
    <% end %>
  <br>
  <div class="btn-toolbar">
    <a href="/courses/new"><button class="btn btn-primary btn-sm btn-space">Create A New Course</button></a>
    <a href="/recent"><button class="btn btn-primary btn-sm btn-space">See Your Recent Rounds</
  </div>
</div>